{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "alibabacloud/sdk": "^1.8",
        "alphasnow/opencc-php": "dev-master",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.12",
        "guzzlehttp/guzzle": "^6.3",
        "laravel-notification-channels/twilio": "^3.0",
        "laravel/framework": "^7.0",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.0",
        "lcobucci/jwt": "^4.1",
        "overtrue/laravel-wechat": "~5.0",
        "socialiteproviders/apple": "^5.0",
        "socialiteproviders/github": "^4.1",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/weixin": "^4.0",
        "socialiteproviders/weixin-web": "^4.1"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "easywechat-composer/easywechat-composer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/alphasnow/opencc-php.git"
        }
    ]
}
